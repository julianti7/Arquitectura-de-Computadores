/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,
		out=NotS);
	And(a=a,
		b=NotS,
		out=T1);
	And(a=b,
		b=sel,
		out=T2);
	Or(a=T1,
		b=T2,
		out=out);
}